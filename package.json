{
  "name": "ftisland",
  "version": "1.0.0",
  "main": "lib/index.js",
  "license": "MIT",
  "scripts": {
    "copy-html": "copyfiles -u 1 src/**/*.html src/**/*.css lib",
    "replace": "rexreplace \"\\.ts\" \".js\" lib/**/*.html",
    "build": "tsc && yarn run copy-html && yarn run replace",
    "prestart": "yarn build",
    "start": "electron ."
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^1.11.0",
    "@typescript-eslint/parser": "^1.11.0",
    "eslint": "^6.0.1",
    "typescript": "^3.5.2"
  },
  "dependencies": {
    "copyfiles": "^2.1.0",
    "electron": "^11.5.0",
    "rexreplace": "^4.1.1"
  }
}
